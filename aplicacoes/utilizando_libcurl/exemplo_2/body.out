  <script type="text/javascript">
  function findPos(bId, sum){
    var x=0, y=0;
    while(bId.parentNode.id!="centraliza"){
      x+=bId.offsetLeft;
      y+=bId.offsetTop;
      bId = bId.parentNode;
    }
    x+=bId.offsetLeft;
    y+=bId.offsetTop;
    return x+"x"+y;
  }
  function placePos(bId, pos){
    var p = pos.split("x");
  	bId.style.left=p[0]+"px";
  	bId.style.top=p[1]+"px";
  }
  // posiciona banners //
  var DEsite="uolbr";
  var DEchan="homeuol";
  var DEsubc="capa";
  var Expble=0;
  var DEt=new Date(); DEt=DEt.getTime(); DErand=Math.floor(DEt*1000*Math.random());
  var scw=0,sch=0; if(screen.height){scw=screen.width;sch=screen.height;}
  function DEshow(ad,pos){
  var DEconn=d.body; DEconn.style.behavior='url(#default#clientCaps)'; DEconn=(DEconn.connectionType=='lan')?1:0;
  if($('banner-'+ad+'-'+pos+'-area')){
  	d.write('<'+'scr'+'ipt type="text/javascript" src="http://bn.uol.com.br/js.ng/site='+DEsite+'&amp;chan='+DEchan+'&amp;subchan='+DEsubc+'&amp;affiliate='+DEsite+DEchan+'&amp;size='+ad+'&amp;page='+pos+'&amp;conntype='+DEconn+'&amp;expble='+Expble+'&amp;reso='+scw+'x'+sch+'&amp;tile='+DErand+'?"><\/scr'+'ipt>');
  	if(ad!='1x1'&&ad!='100x100') 
      setTimeout(function(){
        placePos($('banner-'+ad+'-'+pos), findPos($('banner-'+ad+'-'+pos+'-area'))); // se não é popup, posiciona na tela
      },1000);
  	}
  }

// pop radio 
function pop(url) {
clk = url.replace(/(.*&u=).*/,"$1");
canal = url.replace(/.*canal=(.*)/,"$1");
if(arguments[0].indexOf('abreradio.htm')!=-1) {
	url = clk+"http://app.radio.musica.uol.com.br/radiouol/player/frameset.php?opcao=playlist&nomeplaylist="+canal;
	window.open(url,'popplaylist','width=480,height=570,toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=no');
} else if (arguments[0].indexOf('player.htm')!=-1) {
	player = ((arguments[1].indexOf("_128")!=-1) ? arguments[1].slice(0,-4) : arguments[1].slice(0,-3));
	url = clk+"http://radio.musica.uol.com.br/players/detector.htm?au="+arguments[1]+"&htm="+player+"_player";
	feat = 'toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=no,title=yes,resizable='+((navigator.appName.indexOf("Explorer")!=-1) ? 'no' : 'yes')+',width=215,height=380';
	window.open(url,'audio_'+arguments[1],feat);
} else if (arguments[0].indexOf('frameset.php')!=-1) {
	window.open(url,'popplaylist','width=480,height=570,toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=no');
} else if (arguments[0].indexOf('cdcapa.php')!=-1) {
	try{
		var album = url.match(/album=([^&]+)/)[1];
		var codcd = url.match(/codcd=([^&]+)/)[1];
	} catch(e) {	return false;	}
	url = clk+"http://app.radio.musica.uol.com.br/radiouol/player/frameset.php?opcao=umcd&nomeplaylist="+codcd+"%3C@%3E"+album;
	window.open(url,'popplaylist','width=480,height=570,toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=no');
} else {
	return false; //uolhome
} }

// click uol //
divs=d.getElementsByTagName('div').length;
reF='';
for(i=0;i<divs;i++){
diV=d.getElementsByTagName('div')[i];
dId=diV.id;
if(dId=='menu'||dId=='extra'||dId=='conteudo'||dId=='area-inf'||dId=='barra'||diV.className=='mod'||dId=='hn'||dId=='barra-uol1'||dId=='barra-uol2'||dId=='shop1'||dId=='shop2'||dId=='hUOLfooter'||dId=='hUOLfooter2'||dId=='hnpan2007'||dId=='hn-r'||dId=='arquivohome'||dId=='ombudsman'||dId=='erratas'||dId=='album'||dId=='jornais'||dId=='blogs'||dId=='tms-noticias'||dId=='barraUOL'||dId=='mod-shopping'){
	lnks=diV.getElementsByTagName('a').length;
	for(j=0;j<lnks;j++){
		aObj=diV.getElementsByTagName('a')[j];
		aLnk=aObj.href.toString();
		reF='home';
		if(aObj.name!='') reF+='-'+(dId=="shop1"||dId=="shop2"?"coluna"+(parseFloat(aObj.name)<15?"1":"2")+"-of":"")+aObj.name;
    if (aObj.name == 'hu-home-rotativo') reF = aObj.name;
		if(aLnk.indexOf('http://click.uol')==-1 && aLnk.indexOf('void(')==-1) {
			aObj.href = aLnk.replace(/http(s?):\/\//,'http:\/\/click.uol.com.br/?rf='+reF+'&u=http$1:\/\/');
		}
		if(aLnk.indexOf('radio.musica.uol.com.br')!=-1&&(aLnk.indexOf('player.htm')!=-1||aLnk.indexOf('abreradio.htm')!=-1||aLnk.indexOf('cdcapa.php')!=-1||aLnk.indexOf('frameset.php')!=-1)){
			aObj.href='javascript:pop("'+aObj.href+'")';
		}
	}
} }
