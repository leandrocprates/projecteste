// busca //
var ultimaBarraSel = $('abas').getElementsByTagName('a')[0];
var docForm = $('form');
function UOLBusca(s){
if(ultimaBarraSel!="" && ultimaBarraSel!=s){
	ultimaBarraSel.className="";
}
if(s.nextSibling.innerHTML!=""){
	docForm.getElementsByTagName('select')[0].style.display='none';
	docForm.getElementsByTagName('input')[0].className='texto';
}
s.blur();
s.className="sel";
ultimaBarraSel=s;
d.formb.group.disabled="disabled";
d.formb.skin.disabled=d.formb.id.disabled=d.formb.rd.disabled=(s.innerHTML=='Notícias') ? "" : "disabled";
if(s.innerHTML=='Web') {d.formb.action = "http://busca.uol.com.br/www/index.html"; ababusca = "web";}
else if(s.innerHTML=='Notícias') {d.formb.action = "http://noticias.busca.uol.com.br/uol/index.html"; ababusca = "noticias";}
else if(s.innerHTML=='Imagens') {d.formb.action = "http://busca.uol.com.br/imagem/index.html"; ababusca = "imagens";}
else if(s.innerHTML=='Vídeos') {d.formb.action = "http://busca.uol.com.br/video/index.html"; ababusca = "videos";}
else if(s.innerHTML=='Dicionários') {
	d.formb.action = "http://dic.busca.uol.com.br/result.html";
	d.formb.group.disabled = "";
	d.formb.t.disabled = "";
	d.formb.t.value="10";
	docForm.getElementsByTagName('select')[0].style.display='block';
	docForm.getElementsByTagName('input')[0].className='texto idiomas-texto';
	ababusca = "dicionarios";
}
else if(s.innerHTML=='Comparar preços') { d.formb.action = "http://shopping.busca.uol.com.br/busca.html";  ababusca = "precos" }
docForm.getElementsByTagName('input')[0].focus();
buscatop(ababusca);
}
function buscatop(aba) {
	var abaclick = document.createElement('img');
	document.getElementsByTagName('body')[0].appendChild(abaclick);
	abaclick.src = 'http://click.uol.com.br/?rf=hu-busca-'+aba+'&u=http://home.img.uol.com.br/h3/x.gif';
}
var clickCount = function(clickId){
	var imgClick = document.createElement("img");
	imgClick.id="clickUOL"+clickId;
	document.getElementsByTagName("body")[0].appendChild(imgClick);
	imgClick.src = "http://click.uol.com.br/?rf=home-"+clickId+"&u=http://home.img.uol.com.br/h3/x.gif";
}
var submitEmailForm = function(){
	clickCount('email-caixa');
	checaClickUOLEmail(); 
	return false;
}
var checaClickUOLEmail = function(){
	if(document.getElementById("clickUOLemail-caixa").complete){
		document.formEmail.onsubmit = function(){return true;};		
		document.formEmail.submit();
	}
	else{
		setTimeout(function(){checaClickUOLEmail();},500);
	}
}
var submitEmpregosForm = function(){
	if (document.formEmpregos.sltEmpregos.value!=""){
		clickCount('empregos-caixa');
		checaClickUOLEmpregos();
	}
	return false;
}
var checaClickUOLEmpregos = function(){
  if(document.getElementById("clickUOLempregos-caixa").complete){
    document.location="http://clicklogger.rm.uol.com.br/?prd=21&grp=src:10;size:6;chn:0;creative:caixadebuscahomeuol;thm:"+document.formEmpregos.sltEmpregos.options[document.formEmpregos.sltEmpregos.selectedIndex].id+"&msr=Cliques%20de%20Origem:1&oper=7&redir=http://empregocerto.uol.com.br/search/jobs.html?workingArea="+document.formEmpregos.sltEmpregos.value;
    }
    else{
        setTimeout(function(){checaClickUOLEmpregos();},500);
    }
}

function defineHome(obj){
    var defineHomeClick = document.createElement('img')
    defineHomeClick.src = 'http://click.uol.com.br/?rf=hu-homepage&u=http://home.img.uol.com.br/x.gif';
